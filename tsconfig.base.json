{
    "compileOnSave": false,
    "compilerOptions": {
        "declaration": false,
        "experimentalDecorators": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitOverride": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noPropertyAccessFromIndexSignature": true,
        "removeComments": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true
    }
}
